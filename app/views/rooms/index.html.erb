<section id="content">
  <article id="text">
<p id="notice"><%= notice %></p>

<h1>Rooms</h1>

<table>
  <thead>
    <tr>
      <th>Tite</th>
      <th>Code</th>
      <th>Buildingid</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @rooms.each do |room| %>
      <tr>
        <td><%= room.tite %></td>
        <td><%= room.code %></td>
        <td><%= room.building_id %></td>
        <td><%= link_to 'Show', building_room_path(@building, room) %></td>
        <td><%= link_to 'Edit', edit_building_room_path(@building, room) %></td>
        <td><%= link_to 'Destroy', [@building,room], method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Room', new_building_room_path(@building) %>
  </article>
</section>